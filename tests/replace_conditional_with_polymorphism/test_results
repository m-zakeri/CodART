public class SwitchDemo {
    public int myMethod() {

        int mnth = 8;
        String monthString;
        SwitchDemo_8 chosen = new SwitchDemo_8()
		chosen.myMethod(this);
		System.out.println ( monthString )  ;
		}
			}
			abstract class parentSwitchDemo
{
	public parentSwitchDemo(){
	}
	abstract public void myMethod(SwitchDemo input_class ) ;
}
class SwitchDemo_1 extends parentSwitchDemo
{
	public SwitchDemo_1(){
	}
	public int myMethod(SwitchDemo input_class ) {
		input_class.monthString="January" ;
		ints=2 ;
		break ;
	
	}
}
